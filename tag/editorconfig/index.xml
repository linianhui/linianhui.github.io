&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>trim_trailing_whitespace = &lt;span style="color:#a31515">true&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>
