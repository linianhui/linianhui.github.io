&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#00f">[*.cshtml]&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>end_of_line = &lt;span style="color:#a31515">crlf&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>indent_size = &lt;span style="color:#a31515">4&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#00f">[*.xaml]&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>end_of_line = &lt;span style="color:#a31515">crlf&lt;/span>
